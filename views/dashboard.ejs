<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Dashboard</title>
    <link rel="stylesheet" href="/styles/dashboard.css">
</head>
<body>
    <div class="container">
        <h1>Dashboard</h1>
        <div class="filters">
            <button id="filter-submitted_pending">Incomplete</button>
            <button id="filter-submitted_paid">Paid</button>
            <button id="filter-processing">Processing</button>
            <button id="filter-completed_pending">Completed</button>
            <button id="filter-completed_shipped">Shipped</button>
            <button id="filter-completed_delivered">Delivered</button>
            <input type="text" id="search" placeholder="Search by name">
        </div>
        <table>
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Order Number</th>
                    <th>Order Total</th>
                    <th>Picture Count</th>
                    <th>Box Included?</th>
                    <th>Order Status</th>
                </tr>
            </thead>
            <tbody id="orders">
                <!-- Orders will be loaded here -->
            </tbody>
        </table>
    </div>
    <script src="/scripts/dashboard.js"></script>
</body>
</html>









